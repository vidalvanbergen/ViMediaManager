#tag WindowCode
	#tag Event
		Sub Close()
		  Preferences.SaveWindowPos( Self, "NS wndPreferences" )
		  
		  Preferences.Prefs.setStringValue "RenameStringMovies",     txtRenamePattern(0).Text
		  Preferences.Prefs.setStringValue "RenameStringMovieFiles", txtRenamePattern(4).Text
		  
		  Preferences.Prefs.setStringValue "RenameStringTVShows",    txtRenamePattern(1).Text
		  Preferences.Prefs.setStringValue "RenameStringEpisodes",   txtRenamePattern(3).Text
		  
		  Preferences.Prefs.setStringValue "RenameStringAnime",      txtRenamePattern(2).Text
		  
		  
		  Preferences.Prefs.integerForKey( "MaxThumbs" )    = Val( edtExtraThumbsCount.Text )
		  Preferences.Prefs.integerForKey( "MaxBackdrops" ) = Val( edtExtraFanartsCount.Text )
		  Preferences.Prefs.integerForKey( "MaxActors" )    = Val( edtActorsCount.Text )
		  Preferences.Prefs.integerForKey( "TimeOut" )      = Val( edtTimeOut.Text )
		  
		  Preferences.WriteDefaults
		  
		  'Close
		  
		  'If RefreshList Then CommonCore.ReloadList
		  
		End Sub
	#tag EndEvent

	#tag Event
		Function ConstructToolbar(Toolbar as NSToolbar) As Boolean
		  
		  // Default toolbar settings
		  Toolbar.AllowsUserCustomization = True
		  Toolbar.AutosavesConfiguration = True
		  Toolbar.DisplayMode = NSToolbar.NSToolbarDisplayMode.NSToolbarDisplayModeIconAndLabel 'NSToolbarDisplayModeIconOnly
		  
		  
		  // This is an array of items identifiers to specify the default items (and order) in toolbar
		  // this template is used the first time the app is launched and for defining the default set in toolbar customization
		  Toolbar.DefaultItems = Array( _
		  NSToolbarFlexibleSpaceItemIdentifier, _
		  "PrefGeneral", "PrefExtras", "PrefMovies", "PrefTVShows", "PrefAnime", "PrefServices", _
		  NSToolbarFlexibleSpaceItemIdentifier )
		  Toolbar.SelectableItems = Array( "PrefGeneral", "PrefExtras", "PrefMovies", "PrefTVShows", "PrefAnime", "PrefServices" )
		  
		  
		  // This is an array of items identifiers to specify all the allowed items in toolbar
		  // this template is used to populate the toolbar customization panel
		  Toolbar.AllowedItems = Array ( _
		  _ // Commonly used toolbar items
		  NSToolbarSpaceItemIdentifier, _
		  NSToolbarFlexibleSpaceItemIdentifier, _
		  NSToolbarCustomizeToolbarItemIdentifier, _
		  NSToolbarSeparatorItemIdentifier, _
		  _ // Your toolbar items
		  "PrefGeneral", "PrefExtras", "PrefMovies", "PrefTVShows", "PrefAnime", "PrefServices" )
		  
		  
		  // Add toolbar items
		  'Toolbar.AddControl( PushButton1, "Pushbutton" ) // Simplified method to add customtoolbar items with controls
		  'Toolbar.AddButton( "mainAdvanced", NSImage.Advanced, "Advanced", "", "", False )
		  
		  'dim pic as Picture = template_images
		  
		  Toolbar.AddButton( "PrefGeneral", NSImage.LoadByName( "template_preferences" ), Loc.PrefsGeneral, "", "", True )
		  Toolbar.AddButton( "PrefExtras", NSImage.LoadByName( "template_images" ), Menu.ToolsExtras, "", "", True )
		  Toolbar.AddButton( "PrefMovies", NSImage.LoadByName( "template_movie" ), Loc.Movies, "", "", True )
		  Toolbar.AddButton( "PrefTVShows", NSImage.LoadByName( "template_television" ), Loc.TVShows, "", "", True )
		  Toolbar.AddButton( "PrefAnime", NSImage.LoadByName( "template_anime" ), Loc.Anime, "", "", True )
		  Toolbar.AddButton( "PrefServices", NSImage.LoadByName( "template_services" ), Loc.kServices, "", "", True )
		  
		  toolbar.SelectedItemIdentifier = "PrefGeneral"
		  
		  Return True
		End Function
	#tag EndEvent

	#tag Event
		Function KeyDown(Key As String) As Boolean
		  If Keyboard.AsyncCommandKey Then
		    
		    // Switch Tabs
		    If Asc(Key) >= 49 And Asc(Key) <= 54 And ppPrefs.Value <> Val( Key ) -1 Then
		      'ToolButton( tbPrefs.Item( Val( Key ) ) ).Pushed = True
		      
		      Select case val(key)-1
		      case 0
		        toolbar.SelectedItemIdentifier = "PrefGeneral"
		      case 1
		        toolbar.SelectedItemIdentifier = "PrefExtras"
		      case 2
		        toolbar.SelectedItemIdentifier = "PrefMovies"
		      case 3
		        toolbar.SelectedItemIdentifier = "PrefTVShows"
		      case 4
		        toolbar.SelectedItemIdentifier = "PrefAnime"
		      case 5
		        toolbar.SelectedItemIdentifier = "PrefServices"
		      End Select
		      
		      ppPrefs.Value = Val( Key ) - 1
		      Return True
		    End If
		    
		  End If
		End Function
	#tag EndEvent

	#tag Event
		Sub Open()
		  Dim i as Integer = self.Width
		  Preferences.LoadWindowPos( Self, "NS wndPreferences" )
		  self.Width = i
		  
		  dim SelectedTab as Integer = Preferences.Prefs.integerForKey("NSWindow Preferences Tab")
		  If SelectedTab > -1 then
		    ppPrefs.Value = SelectedTab
		    'ToolButton( tbPrefs.Item( Preferences.Prefs.integerForKey("NSWindow Preferences Tab") + 1 ) ).Pushed = True
		    
		    Select case Preferences.Prefs.integerForKey("NSWindow Preferences Tab")
