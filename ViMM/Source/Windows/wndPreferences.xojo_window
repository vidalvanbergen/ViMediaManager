#tag Events btnTraktTVTest
	#tag Event
		Sub Action()
		  Dim sckt as New HTTPSocket
		  sckt.Yield = True
		  
		  Dim jItem as New JSONItem
		  
		  Try
		    sckt.SetRequestContent( "{ ""username"": """ + edtTraktTVUserName.Text + """, ""password"": """ + edtTraktTVPassword.Text + """ }", "" )
		    jItem.Load( sckt.Post( URLCore.TraktTV( "Test" ), 30 ) )
		  Catch err as JSONException
		    Break
		  End Try
		  
		  Preferences.Prefs.boolForKey("TraktTVEnabled") = ( jItem <> Nil And jItem.HasName("status") And jItem.Value("status") = "success" )
		  lstServices.Refresh
		  
		  if Preferences.Prefs.boolForKey("TraktTVEnabled") then
		    MsgBox "You have successfully logged in to Trakt.tv"
		  Else
		    MsgBox "Username or Password incorrect"
		  End If
		End Sub
	#tag EndEvent
#tag EndEvents
