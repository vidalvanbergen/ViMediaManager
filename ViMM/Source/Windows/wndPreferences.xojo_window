		        end if
		      Case "ExtraFanart"
		        if Globals.IsDarkMode then
		          ico = extrafanartactive
		        else
		          If Me.Selected( row ) Then Ico = extrafanartactive Else Ico = extrafanartinactive
		        end if
		      Case "SeasonArt"
		        if Globals.IsDarkMode then
		          ico = extrathumbsactive
		        else
		          If Me.Selected( row ) Then Ico = extrathumbsactive Else Ico = extrathumbsinactive
		        end if
		        
		      Case "ClearArt"
		        if Globals.IsDarkMode then
		          ico = clearartactive
		        else
		          If Me.Selected( row ) Then Ico = clearartactive Else Ico = clearartinactive
		        end if
		      Case "CharacterArt"
		        if Globals.IsDarkMode then
		          ico = characteractive
		        else
		          If Me.Selected( row ) Then Ico = characteractive Else Ico = characterinactive
		        end if
		      Case "DiscArt"
		        if Globals.IsDarkMode then
		          ico = discartactive
		        else
		          If Me.Selected( row ) Then Ico = discartactive Else Ico = discartinactive
		        end if
		      Case "Logo"
		        if Globals.IsDarkMode then
		          ico = logoactive
		        else
		          If Me.Selected( row ) Then Ico = logoactive Else Ico = logoinactive
		        end if
		      Case "Thumb"
		        if Globals.IsDarkMode then
		          ico = landscapeactive
		        else
		          If Me.Selected( row ) Then Ico = landscapeactive Else Ico = landscapeinactive
		        end if
		      Case "EpisodeThumb"
		        if Globals.IsDarkMode then
		          ico = landscapeactive
		        else
		          If Me.Selected( row ) Then Ico = landscapeactive Else Ico = landscapeinactive
		        end if
		        
		      Case "ActorThumbs"
		        if Globals.IsDarkMode then
		          ico = clearartactive
		        else
		          If Me.Selected( row ) Then Ico = clearartactive Else Ico = clearartinactive
		        end if
		        
		      Case "Trailer"
		        if Globals.IsDarkMode then
		          ico = traileractive
		        else
		          If Me.Selected( row ) Then Ico = traileractive Else Ico = trailerinactive
		        end if
		      Case "Themesong"
		        if Globals.IsDarkMode then
		          ico = themesongactive
		        else
		          If Me.Selected( row ) Then Ico = themesongactive Else Ico = themesonginactive
		        end if
		      Case "Subtitles"
		        if Globals.IsDarkMode then
		          ico = subtitlesactive
		        else
		          If Me.Selected( row ) Then Ico = subtitlesactive Else Ico = subtitlesinactive
		        end if
		      Case "Resolution"
		        if Globals.IsDarkMode then
		          ico = def720pactive
		        else
		          If Me.Selected( row ) Then Ico = def720pactive Else Ico = def720pinactive
		        end if
		        
		      End Select
		      
		      'MessageBox Str( App.ScalingFactor )
		    End If
		    
		    
		    If column = 0 Then g.ScaleImage( Ico, 20, 20, 2, 2 )
		    
		    If column = 3 And ( Me.RowTag( row ) = "Poster" Or Me.RowTag( row ) = "Backdrop" OR Me.RowTag( row ) = "Trailer" ) Then
		      g.ScaleImage( dropdown, 14, 14, g.Width - 14 - 6, 4 )
		    End If
		    
		    If column = 4 Then
		      If Me.RowTag( row ) = "Poster" OR Me.RowTag( row ) = "Backdrop" OR Me.RowTag( row ) = "banner" OR Me.RowTag( row ) = "Trailer" Then
		        g.ScaleImage( dropdown, 14, 14, g.Width - 14 - 6, 4 )
		      End If
		    End If
		    
		    Return True
		  End If
		End Function
