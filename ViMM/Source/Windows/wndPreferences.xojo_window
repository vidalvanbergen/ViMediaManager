	#tag Method, Flags = &h1
		Protected Sub AddToMediaList(Index as Integer)
		  Dim dlg as new SelectFolderDialog
		  Dim f as FolderItem = dlg.ShowModal
		  Dim lstbox as Listbox = lstMedia( index )
		  
		  If f = Nil then Return
		  lstbox.AddRow f.Name, f.ShellPath.ReplaceAll("\","")
		  lstbox.RowPicture( lstbox.LastIndex ) = f.FileIcon(16)
		  
		  AddMediaToPrefs index
		  RefreshList = True
		  
		  Exception err as NilObjectException
		    MsgBox Loc.FileNotFound
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h1
		Protected Sub RemoveFromMediaList(Index as Integer)
		  If lstMedia(index).ListIndex > -1 then
		    lstMedia(index).RemoveRow lstMedia(Index).ListIndex
		    AddMediaToPrefs index
		    RefreshList = True
		  End If
		End Sub
	#tag EndMethod


