#tag Events popTVDLFolder
	#tag Event
		Sub Open()
		  
		  Dim f as FolderItem = Preferences.Prefs.fileForKey( "TVDownloads" )
		  
		  If f <> Nil And f.Exists Then
		    Me.AddRow f.Name
		    Me.RowTag( Me.ListCount -1 ) = f'.ShellPath
		    #If TargetCocoa Then
		      dim icon as MacIcon = MacIcon.NewIconFromFolderItem(f)
		      'icon.Size = 16
		      Me.Icon( Me.ListCount -1 ) = new NSImage( icon ) 'New NSImage( f.Icon( 16 ) )
		    #endif
		    
		  End If
		  
		  Me.AddRow "-"
		  Me.AddRow Loc.None
		  Me.AddRow kSelectFolder
		  
		  If f <> Nil And f.Exists Then Me.ListIndex = 0 Else Me.ListIndex = 1
		End Sub
	#tag EndEvent
	#tag Event
		Sub Change()
		  
		  If Me.Text = Loc.None Then
		    Preferences.Prefs.setFileValue("TVDownloads", Nil )
		    'Return
		  ElseIf Me.Text = kSelectFolder Then
		    
		    Dim f as FolderItem = SelectFolder()
		    
		    If f <> Nil And f.Exists Then
		      Preferences.Prefs.setFileValue("TVDownloads", f )
		      
		      Me.InsertRow( 0, f.Name )
		      Me.RowTag( 0 ) = f
		      #If TargetCocoa Then
		        dim icon as MacIcon = MacIcon.NewIconFromFolderItem(f)
		        'icon.Size = 16
		        Me.Icon( 0 ) = New NSImage( icon )
		      #endif
		      Me.ListIndex = 0
		    Else
		      
		      For i as Integer = 0 to Me.ListCount - 1
		        If Me.List(i) = Loc.None Then
		          Me.ListIndex = i
		          Exit
		        End If
		      Next
		      
		    End If
		    
		    'Return
		  Else
		    Dim f as FolderItem = Me.RowTag( Me.ListIndex )
		    Preferences.Prefs.setFileValue("TVDownloads", f )
		    
		    'Return
		  End If
		End Sub
