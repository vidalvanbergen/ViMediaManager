n.CreateAsFolder		          		          For i as Integer = 0 to Images.Child("backdrops").Count - 1		            Dim jItem as new JSONItem		            jItem = Images.Child("backdrops").Child(i)		            		            If NOT CommonCore.MassSearch Then		              dlgProgress.Add( Loc.kDownloading + ":", str(i+1) + Loc.k1of2 + Str( Images.Child("backdrops").Count ) + " " + Info + " " + Loc.kFanart.Lowercase + " " + Loc.kImages.Lowercase + "...", 1, Images.Child("backdrops").Count )		            Else		              dlgProgress.Debug( str(i+1) + Loc.k1of2 + Str( Images.Child("backdrops").Count ) + " " + Info + " " + Loc.kFanart.Lowercase + " " + Loc.kImages.Lowercase + "..." )		            End If		            Dim FanartName() as String = jItem.Value("file_path").split("/")		            Pic = URL2Picture( URLbase_image + SizeFanart + jItem.Value("file_path"), FanartName( FanartName.Ubound ).ReplaceAll(".jpg", ""), "jpg", Destination ) '"fanart" + str(i+1), "jpg", Destination )		            Pic = Nil		          Next		        End If		        		      End If		    End If // @END Images.HasName("backdrops")		    		    // ## FanartTV		    If Prefs.boolForKey("AutoDownloadClearArt") Or Prefs.boolForKey("AutoDownloadLogo") Or Prefs.boolForKey("AutoDownloadDiscArt") Or Prefs.boolForKey("AutoDownloadBanner") Or Prefs.boolForKey("AutoDownloadThumb") Then		      If CommonCore.MassSearch Then dlgProgress.Debug( Loc.kDownloading + " " + Info + " " + Loc.kExtraArt.Lowercase + "..." )		      		      Dim f as FolderItem = FindImageBanner( MovieAttr.FolderParent )		      		      Dim Count as Integer = 0		      If Prefs.boolForKey("AutoDownloadClearArt") And _		        ( FanartTVJSON.HasName("movieart") Or FanartTVJSON.HasName("hdmovieart") ) And _		        NOT MovieAttr.FolderParent.Child("clearart.png").Exists Then Count = Count + 1		        		        If Prefs.boolForKey("AutoDownloadLogo")     And _		          ( FanartTVJSON.HasName("movielogo") Or